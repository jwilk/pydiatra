[MASTER]
load-plugins = pylint.extensions.check_elif

[MESSAGES CONTROL]
disable =
    bad-builtin,
    bad-continuation,
    bad-option-value,
    consider-using-f-string,
    duplicate-code,
    fixme,
    inconsistent-return-statements,
    invalid-name,
    locally-disabled,
    locally-enabled,
    no-else-continue,
    no-else-return,
    no-self-use,
    raise-missing-from,
    redefined-variable-type,
    super-with-arguments,
    too-few-public-methods,
    too-many-boolean-expressions,
    too-many-branches,
    too-many-locals,
    too-many-return-statements,
    too-many-statements,
    useless-object-inheritance,

[BASIC]
no-docstring-rgx = .*

[REPORTS]
reports = no
score = no
msg-template = {path}:{line}: {C}: {symbol} [{obj}] {msg}

[FORMAT]
max-line-length = 120
expected-line-ending-format = LF

